Hostname    "<%= node[:hostname] %>"
TypesDB     "/usr/share/collectd/types.db"

LoadPlugin syslog
LoadPlugin cpu
LoadPlugin interface
LoadPlugin load
LoadPlugin memory
LoadPlugin write_http

Include "/etc/collectd.d"

<Plugin write_http>
  <URL "<%= node[:librato][:url] %>">
    User "<%= node[:librato][:email] %>"
    Password "<%= node[:librato][:api_key] %>"
    Format "JSON"
  </URL>
</Plugin>
